  
public class savingAccount extends BankAccount {
  String accName = "Saving Account";
	int accNumb, numWithdraws;
	boolean status;

	public savingAccount()
	{
		numWithdraws = 0;
		balance = 0;
		status = true;
		AIR = 0;
		type = "Savings";
	}

	double balance, penalty = 0, MI, MIR, AIR;

	@Override
	public void monthlyProcess() 
	{
		balance = getBalance();
		if (balance<0)
			balance = 0;
		else if (balance < 100) 
		{
			setStatus(false);
			if (balance == 0)
				penalty += 1;
		}
		if (numWithdraws > 4)
			penalty += (numWithdraws - 4);
		balance -= penalty;

		MIR = (AIR / (12 * 100));
		MI = balance * MIR;
		balance += MI;
		setBalance(balance);
	}

	public double getAIR() {
		return this.AIR;
	}

	public void setAIR(double AIR) {
		this.AIR = AIR;
	}

	public String toString() {
		return "accName " + accName + "\n" + "accNumb " + accNumb + "\n"
				+ "numWithdraws " + numWithdraws + "\n" + "balance " + balance
				+ "\n" + "status " + status;
	}

}
